{
  "name": "potolki-sokol-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href=/potolki-admin/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "lint": "ng lint; stylelint \"**/*.{css,scss}\"",
    "lint:fix": "ng lint --fix; stylelint --fix \"**/*.{css,scss}\"",
    "format": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.6",
    "@angular/cdk": "19.1.4",
    "@angular/common": "19.1.6",
    "@angular/compiler": "19.1.6",
    "@angular/core": "19.1.6",
    "@angular/forms": "19.1.6",
    "@angular/platform-browser": "19.1.6",
    "@angular/platform-browser-dynamic": "19.1.6",
    "@angular/router": "19.1.6",
    "@nebular/eva-icons": "15.0.0",
    "@nebular/theme": "15.0.0",
    "@ngrx/operators": "19.1.0",
    "@ngrx/signals": "19.1.0",
    "@supabase/supabase-js": "2.49.4",
    "eva-icons": "1.1.3",
    "ng-gallery": "12.0.0",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.6",
    "@angular/cli": "19.1.6",
    "@angular/compiler-cli": "19.1.6",
    "@ngx-env/builder": "^19.0.4",
    "@schematics/angular": "19.1.6",
    "@types/jasmine": "5.1.0",
    "angular-cli-ghpages": "2.0.3",
    "angular-eslint": "19.3.0",
    "eslint": "9.23.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "9.1.7",
    "jasmine-core": "5.6.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.5.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.5.3",
    "prettier-plugin-organize-attributes": "1.0.0",
    "stylelint": "16.17.0",
    "stylelint-config-property-sort-order-smacss": "10.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.11.1",
    "typescript": "5.7.2",
    "typescript-eslint": "8.27.0"
  },
  "lint-staged": {
    "*.{ts,js,html,json,yml,md}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
